echo -e "${B}	        └─────────────╯──╮"
echo -e "${B}         │${red}3${B}│───│${white} Paginas Blancas${B}│"
echo -e "${B}               └────────────────╯${white}"
echo -e "${B}         │${red}4${B}│───│${white} BeenVerified${B}│"
echo -e "${B}               └─────────────╯${white}"
echo -e "${B}         │${red}5${B}│───│${white} DNI${B} │"
echo -e "${B}               └─────╯─────────╮${white}"
echo -e "${B}         │${red}6${B}│───│${white} CheckUsernames${B}│"
echo -e "${B}               └───────────────╯${white}"
echo -e "${B}         │${red}7${B}│───│${white} BeenVerifield${B} │"
echo -e "${B}               └───────────────╯${white}"
echo -e "${B}         │${red}8${B}│───│${white} Deteas${B} │"
echo -e "${B}               └────────╯${white}"
echo -e "${B}         │${red}9${B}│───│${white} Tineye${B} │"
echo -e "${B}               └────────╯──╮${white}"
echo -e "${B}         │${red}10${B}│──│${white} NumVerify${B} │"
echo -e "${B}               └───────────╯${white}"
echo -e "${B}         │${red}11${B}│──│${white} Curp${B} │"
echo -e "${B}               └──────╯────╮${white}"
echo -e "${B}         │${red}12${B}│──│${white} IpTracker${B} │"
echo -e "${B}               └───────────╯${white}"
echo -e "${B}         │${red}13${B}│──│${white} IpLoguer${B} │"
echo -e "${B}               └──────────╯${white}"

echo -e "${B}         │${red}14${B}│───│${red} Salir${B} │"
echo -e "${B}                └───────╯${white}"

    echo -ne "${B}[${red}~${B}] ${bold}${white}Doxme ${green}>${white} "
	read opc

    case $opc in
        1) xdg-open https://www.revealname.com/ ;;
	2) xdg-open https://demo.phoneinfoga.crvx.fr/ ;;
	3) xdg-open http://www.paginasblancas.pe/ ;;
