bold="\e[1m"
underline="\e[4m"
dim="\e[2m"
blink="\e[5m"
red="\e[31m"
green="\e[32m"
orange="\e[33m"
blue="\e[34m"
purple="\e[35m"
B="\e[94m"
white="\e[97m"
default="\e[39m"
G="\e[0;32m\033[1m"
R="\e[1;31m"
RED="\e[101m"
green='\e[32m'
W="\033[0m"
GR='\033[90m'
BL="\e[44m"
WI="\e[107m"
B="\e[0;34m\033[1m"

function print_color {
    echo -e "$1$2$normal"
}

opc=0

while [ $opc -ne 14 ] ; do
    clear
echo -e '\e[94m
  ________
  \______ \   _______  ___ _____   ____
   |    |  \ /  _ \  \/  //     \_/ __ \
   |    `   (  <_> >    <|  Y Y  \  ___/
  /_______  /\____/__/\_ \__|_|  /\___  >
          \/            \/     \/     \/'
  echo -e "  	     ${W} ${B}${WI} .:: DOXME ::. ${W}"
